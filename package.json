{
  "name": "math-from-string",
  "description": "Parses strings as mathematical expressions.",
  "main": "dist/math-from-string.js",
  "scripts": {
    "prepare": "npm run dist -s",
    "dist": "npm run test && npm run lint && npm run babel && npm run minify && npm run preamble && npm run changelog",
    "lint": "eslint src/",
    "babel": "babel src/ -o dist/math-from-string.js",
    "minify": "uglifyjs dist/math-from-string.js -mco dist/math-from-string.min.js",
    "preamble": "preamble -p \"/*github.com/Grafluxe/math-from-string*/\n\" -o ./dist/math-from-string.min.js",
    "doc": "jsdoc -c .jsdocrc",
    "test": "node test/runner",
    "changelog": "git-to-changelog"
  },
  "author": "Leandro Silva (http://grafluxe.com)",
  "homepage": "https://github.com/Grafluxe/math-from-string",
  "version": "2.0.0",
  "license": "MIT",
  "repository": "Grafluxe/math-from-string.git",
  "keywords": [
    "Math",
    "Math from string",
    "Math utility",
    "Math helper",
    "Mathematical expressions",
    "Parse math",
    "Parse math from string",
    "Calculate string",
    "Convert string to math",
    "String utility",
    "String util",
    "String format",
    "String formatter",
    "String helper"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-preset-es2015": "^6.18.0",
    "boxy-jsdoc-template": "^2.0.2",
    "eslint": "^4.19.1",
    "git-to-changelog": "^1.2.2",
    "jasmine": "^3.1.0",
    "jasmine-pretty-html-reporter": "^0.2.5",
    "jsdoc": "^3.4.3",
    "preamble-cli": "^2.0.0",
    "uglify-js": "^3.3.18"
  }
}
